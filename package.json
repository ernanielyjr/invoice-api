{
  "name": "myplan-api",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "14.18.1",
    "npm": "6.14.15"
  },
  "main": "src/server.js",
  "scripts": {
    "postinstall": "npm run review && npm run build",
    "start": "nodemon",
    "start:dev": "concurrently --kill-others \"npm run build:watch\" \"npm start\"",
    "copy:templates": "cp -R ./src/templates ./dist",
    "build": "tsc && npm run copy:templates",
    "build:watch": "tsc -w && npm run copy:templates",
    "lint": "tslint --project ./",
    "prettify": "prettier --check \"./src/**/*.{js,ts}\"",
    "review": "npm run lint && npm run prettify",
    "review:fix": "npm run lint -- --fix && npm run prettify -- --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^5.11.5",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.16",
    "pagseguro": "^0.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.3",
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.0.37",
    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/mongoose": "^5.10.2",
    "@types/morgan": "^1.7.33",
    "@types/multer": "^1.3.6",
    "@types/node": "^10.9.4",
    "@types/xml2js": "^0.4.3",
    "concurrently": "^6.1.0",
    "jwt-simple": "^0.5.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^1.7.2",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2"
  }
}
